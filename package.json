{
  "name": "poc-mfe-login",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "build:dev": "ng build --configuration development",
    "start:static": "ng build --configuration development && npx serve dist/poc-mfe-login --listen 4202 --cors"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.7",
    "@angular/router": "^20.3.0",
    "@poc-mfe/shared": "^0.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.22.1",
    "@analogjs/vite-plugin-angular": "^1.22.1",
    "@analogjs/vitest-angular": "^1.22.1",
    "@angular-architects/module-federation": "^20.0.0",
    "@angular/build": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/compiler-cli": "^20.3.0",
    "@testing-library/angular": "^18.1.0",
    "jsdom": "^27.0.1",
    "ngx-build-plus": "^20.0.0",
    "serve": "^14.2.5",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4"
  }
}
